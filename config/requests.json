      "path": "/api/v2/assets",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Basic <%= encode(iparam.api_key + ':X') %>"
      },
      "query": {
        "query": "\"%7E%5Bname%7Cdisplay_name%5D%3A%27<%=asset_query%>%27\""
      }
    }
  },
  "getServices": {
    "schema": {
      "method": "GET",
