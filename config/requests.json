{
  "getRequesters": {
    "schema": {
      "method": "GET",
      "protocol": "https",
      "host": "<%=iparam.freshservice_domain%>",
      "path": "/api/v2/requesters",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Basic <%= encode(iparam.api_key + ':X') %>"
      },
      "query": {
        "query": "\"%7E%5Bfirst_name%7Clast_name%7Cprimary_email%5D%3A%27<%=requester_query%>%27\""
      }
    }
  },
  "getAgents": {
    "schema": {
      "method": "GET",
      "protocol": "https",
