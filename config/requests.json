{
  "getRequesters": {
    "schema": {
      "method": "GET",
      "protocol": "https",
      "host": "<%=iparam.freshservice_domain%>",
      "path": "/api/v2/requesters",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Basic <%= encode(iparam.api_key) %>"
      },
      "query": {
        "query": "{requester_query}"
      }
    }
  },
  "getAgents": {
    "schema": {
